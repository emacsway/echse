args "--unamed-opts --no-handle-error --long-help -a echs_args_info -f echs_parser"
package "echse-collect"
usage "echse-collect [OPTION]... SOURCE..."
description "tba."

option "from" -
	"Start event states at DT."
	string typestr="DT" optional

option "till" -
	"Finish event streams by DT."
	string typestr="DT" optional

option "filter" f
	"Use filter script FILE."
	string typestr="FILE" optional
